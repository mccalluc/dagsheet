<root>
    <mxCell id="2" vertex="1" parent="1">
        <Object formula="&quot;World&quot;" as="value"/>
        <mxGeometry x="20" y="20" width="100" height="30" as="geometry"/>
    </mxCell>
    <mxCell id="3" vertex="1" parent="1">
        <Object formula="function(name){return &quot;Hello &quot;+name+&quot;!&quot;}" as="value"/>
        <mxGeometry x="220" y="20" width="300" height="30" as="geometry"/>
    </mxCell>
    <mxCell id="4" vertex="1" parent="1">
        <Object formula="greet(name)" as="value"/>
        <mxGeometry x="120" y="120" width="150" height="30" as="geometry"/>
    </mxCell>
    <arrow label="name"  from="2" to="4"/>
    <arrow label="greet" from="3" to="4"/>
</root>